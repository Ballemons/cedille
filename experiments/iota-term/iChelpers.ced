module icHelpers.

import nat.
import cast.

biMono ◂ (★ ➔ ★ ➔ ★) ➔ ★ =
 λ F : ★ ➔ ★ ➔ ★.
  ∀ R1 : ★ . ∀ R2 : ★ . ∀ R1' : ★ . ∀ R2' : ★ .
    Cast · R1 · R1' ➔ Cast · R2 · R2' ➔ Cast · (F · R1 · R2) · (F · R1' · R2').

Persistent ◂ (Nat ➔ ★) ➔ ★ =
  λ X : Nat ➔ ★ . Π m : Nat . Π n : Nat . Lte m n ➾ Cast · (X m) · (X n).

