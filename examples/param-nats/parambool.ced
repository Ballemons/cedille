import base.

Bool ◂ ★ = ι x : cBool . ι _ : (x cTrue cFalse ≃ x) . UnaryParamBool x .

True ◂ Bool = [ cTrue, [β{cTrue} , Λ A . Λ R . Λ a . Λ b . λ pa . λ pb . pa ]] .
False ◂ Bool = [ cFalse, [β{cFalse}, Λ A . Λ R . Λ a . Λ b . λ pa . λ pb . pb ]] .

Ind ◂ ∀ Q : Bool ➔ ★ . Q True ➔ Q False ➔ Π x : Bool . Q x =
  Λ Q . λ pt . λ pf . λ x .
    ρ ς x.2.1 - (x.2.2 · Bool · Q -True -False pt pf).

