import nat .

rec Vector (A : ★) : (n : Nat) | Nil : Vector Z , Cons : ∀ n : Nat . A → Vector n → Vector (S n) =
  ∀ P : Π n : Nat . Vector n → ★ .
    (∀ n : Nat . Π a : A . ∀ v : Vector n .
       P n v → P (S n) (Cons n a v)) → 
    P Z Nil → 
    P n self
  with
    Nil = λ P . λ c . λ n . n ,
    Cons = λ n . λ a . λ v . λ P . λ c . λ n . c -n a (v -P c n).


