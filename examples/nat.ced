rec Nat | Z : Nat , S : Nat → Nat =
  ∀ P : Nat → ★ .
    (∀ n : Nat . P n → P (S n)) → P Z → P self
  with
    Z = Λ P . λ s . λ z . z,
    S = λ n . Λ P . λ s . λ z . s -(n · P) (n · P s z) .

plus ⇐ Nat → Nat → Nat = λ n . λ m . n · (λ n : Nat . Nat) (Λ n . S) m .

one ⇐ Nat = S Z .

two ⇐ Nat = S (S Z) .

four ⇐ Nat = plus two two.

plusZ ⇐ Π x : Nat . plus x Z ≃ x = λ x . x · (λ n . plus n Z ≃ n) (Λ n . λ u . ●) β .
