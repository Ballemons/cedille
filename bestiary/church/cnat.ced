import cbool.

cNat ◂ ★ = ∀ X : ★ . X ➔ (X ➔ X) ➔ X .
cZ ◂ cNat = Λ X . λ z . λ s . z .
cS ◂ cNat ➔ cNat = λ n . Λ X . λ z . λ s . s (n · X z s) .

pred ◂ cNat ➔ cNat =
  λ n . Λ X . λ z . λ s . n · ((X ➔ X) ➔ X) (λ u . z) (λ g . λ h . h (g s)) (λ u . u) .

test1 ◂ cNat = pred (cS (cS cZ)) .

eqnat ◂ cNat ➔ cNat ➔ cBool =
  λ n .
    n · (cNat ➔ cBool)
      (λ m . m · cBool tt (λ _ . ff))
      (λ f . λ m . m · cBool ff (λ _ . f (pred m))) .
    
test ◂ cBool = eqnat (cS cZ) (cS cZ) .
