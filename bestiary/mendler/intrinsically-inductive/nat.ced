import ../cnat.

NatP ◂ cNat ➔ ★ =
  λ n : cNat .
    ∀ X : cNat ➔ ★ .
    X cZ ➔
    (∀ A : ★ . ∀ R : A ➔ ★ . ∀ f : A ➔ cNat . (∀ a : A . R a ➔ X (f a)) ➔ ∀ a : A . R a ➔ X (cS · A f a)) ➔
    X n.

Nat ◂ ★ = ι n : cNat . NatP n .

Z ◂ Nat = [ cZ, Λ R . λ z . λ s . z ] .

S ◂ ∀ A : ★ . (A ➔ Nat) ➔ A ➔ Nat =
  Λ A : ★ . λ e . λ d . [ cS · A (λ a . (e a).1) d,
                          Λ X . λ z . λ s . s · A ● ] .
