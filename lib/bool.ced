import false.
import true.

rec Bool | tt : Bool , ff : Bool =
  ∀ P : Bool → ★ .
    P tt → P ff → P self
  with
    tt = Λ P . λ a . λ b . a ,
    ff = Λ P . λ a . λ b . b .

and ⇐ Bool → Bool → Bool = 
  λ x . λ y . θ x y ff .

or ⇐ Bool → Bool → Bool = 
  λ x . λ y . x · (λ b : Bool . Bool) tt y .

not ⇐ Bool → Bool = λ x . θ x ff tt .

Bool-to-type ⇐ Bool → ★ = 
  λ b : Bool . ↑ X . b · (λ b : Bool . X) : (☆ → ☆ → ☆) · True · False .
