module erased-let.
import nat.

-- Example lemma
s-eq-s ◂ Π m : Nat. Π n : Nat. {S m ≃ S n} ➾ {m ≃ n} =
  λ m. λ n. Λ e. χ {pred (S m) ≃ pred (S n)} - ρ e - β.

-- Demonstration of issue
problem ◂ ∀ m : Nat. ∀ n : Nat. {S m ≃ S n} ➾ ∀ Q : Nat ➔ ★. Q m ➔ Q n =
  Λ m. Λ n. Λ e. Λ Q. λ q. [e' ◂ {m ≃ n} = s-eq-s m n -e] - ρ ς e' - q.
--^------------------------------------------------------------------^
-- This erases to λ q. [e' = s-eq-s m n] - q, where m and n are unbound variables!
