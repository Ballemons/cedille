module kind-checking.

T ◂ Π A : ★. Π a : A. Π a' : A. {a ≃ a'} ➔ ★ =
  λ A : ★. λ a : A. λ a' : A. λ e : {a ≃ a'}. ∀ X : A ➔ ★. X a ➔ X a'.

t ◂ ∀ A : ★. ∀ a : A. ∀ e : {a ≃ λ x. x x}. T · A a a β ➔ T · A a a β =
  Λ A. Λ a. Λ e. ρ<1 3 4> e - λ t. Λ X. λ x. x.

T' ◂ {β ≃ β} ➔ ★ = λ a : {β ≃ β}. ∀ X : {β ≃ β} ➔ ★. Π x : X β{λ x. x x}. X a.

True ◂ ★ = ∀ X : ★. X ➔ X.
triv ◂ True = Λ X. λ x. x.

t' ◂ ∀ x : {β ≃ β}. {x ≃ λ x. x x} ➾ T' x ➔ T' β{λ x. x x} =
  Λ x. Λ e. λ t. triv (ρ e - t).
