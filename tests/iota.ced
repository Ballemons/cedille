rec pNat | =
  ∀ X : ★ . (pNat ➔ X ➔ X) ➔ X ➔ X
  with .

pZ ◂ pNat = Λ X . λ s . λ z . z .

pS ◂ pNat ➔ pNat = λ n . Λ X . λ s . λ z . s n (n · X s z) .

Nat ◂ ★ =
  ι x : pNat .
    ∀ P : pNat ➔ ★ . (Π x : pNat . P x ➔ P (pS x)) ➔ P pZ ➔ P x.

Z ◂ Nat = [ pZ , Λ P . λ s . λ z . z ].

S ◂ Nat ➔ Nat = λ n . [ pS n.1 , Λ P . λ s . λ z . s n.1 (n · P s z) ].

pNat-to-Nat ◂ pNat ➔ Nat =
  λ p . p · Nat (λ _ . λ r . S r) Z .

lem ◂ Π x : Nat . pNat-to-Nat x ≃ x =
  λ x . θ x (λ x . λ ih . ρ+ ih - β) β .

P ◂ Nat ➔ Nat = λ n . θ n (λ p . λ _ . pNat-to-Nat p) Z .

add ◂ Nat ➔ Nat ➔ Nat = λ n . λ m . θ n (λ n . S) m .

