module core.test.

False : ★ = ∀ X: ★. X.
Bot : ★ = False.
Not : ★ ➔ ★ = λ A: ★. A ➔ False.

elimBot: ∀ X: ★. False ➔ X = Λ X. λ f. f ·X.
explode = elimBot.

botIrrel : Bot ➾ Bot
= Λ b. δ - b ·{λ x. λ y. x ≃ λ x. λ y. y}.


simple : { λ x . λ y . y ≃ (λ y . y) (λ x . x) } ➔ ∀ X : ★ . X =
  λ pf . δ - pf.

check-test : ∀ X : ★ . X =
  simple β .

check-test2 : ∀ X: ★. ∀ P: X ➔ ★. ∀ x: X. ∀ y: X. P x ➔ P y
= Λ X. Λ P. Λ x. Λ y. λ pf. pf . 
